{
    "rules": {
        "no-invalid-regexp": 1,
        "no-empty": 1,
        "no-native-reassign": 1,
        "no-mixed-spaces-and-tabs": 2,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "html"
    ]
}
